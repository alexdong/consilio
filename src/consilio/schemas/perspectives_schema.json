{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "Title",
      "Expertise",
      "Goal",
      "Role"
    ],
    "properties": {
      "Title": {
        "type": "string",
        "minLength": 1
      },
      "Expertise": {
        "type": "string",
        "minLength": 1
      },
      "Goal": {
        "type": "string",
        "minLength": 1
      },
      "Role": {
        "type": "string",
        "minLength": 1
      }
    }
  },
  "minItems": 1,
  "maxItems": 10
}